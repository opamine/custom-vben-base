{
  "name": "vvt-base",
  "version": "0.0.1",
  "homepage": "https://github.com/anncwb/vue-vben-admin",
  "bugs": {
    "url": "https://github.com/opamine/vvt-base/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/opamine/vvt-base.git"
  },
  "license": "MIT",
  "author": {
    "name": "Dopamine",
    "email": "zzhou998@163.com",
    "url": "https://github.com/opamine"
  },
  "scripts": {
    "bootstrap": "pnpm install",
    "build": "NODE_ENV=production pnpm vite build",
    "build:analyze": "pnpm vite build --mode analyze",
    "build:no-cache": "pnpm clean:cache && npm run build",
    "build:test": "pnpm vite build --mode test",
    "commit": "czg",
    "dev": "pnpm vite",
    "lint:eslint": "eslint --fix --max-warnings 0  \"{src,mock}/**/*.{vue,js,jsx,ts,tsx}\"  --cache --cache-location node_modules/.cache/eslint/",
    "lint:prettier": "prettier --write  .",
    "lint:stylelint": "stylelint --fix \"**/*.{css,less,html,vue}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint:lint-staged": "lint-staged",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install",
    "preview": "npm run build && vite preview",
    "reinstall": "rimraf pnpm-lock.yaml && rimraf package.lock.json && rimraf node_modules && npm run bootstrap",
    "serve": "npm run dev",
    "test:gzip": "npx http-server dist --cors --gzip -c-1",
    "type:check": "vue-tsc --noEmit --skipLibCheck"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "{!(package)*.json,*.code-snippets,.!(browserslist)*rc}": [
      "prettier --write--parser json"
    ],
    "package.json": [
      "prettier --write"
    ],
    "*.vue": [
      "eslint --fix",
      "prettier --write",
      "stylelint --fix"
    ],
    "*.{less,html}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "dependencies": {
    "@ant-design/colors": "^7.0.0",
    "@ant-design/icons-vue": "^6.1.0",
    "@iconify/iconify": "^3.1.0",
    "@vue/runtime-core": "^3.2.47",
    "@vue/shared": "^3.2.47",
    "@vueuse/core": "^9.13.0",
    "@vueuse/shared": "^9.13.0",
    "ant-design-vue": "^3.2.17",
    "axios": "^1.3.4",
    "cropperjs": "^1.5.13",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.7",
    "echarts": "^5.4.2",
    "lodash-es": "^4.17.21",
    "mockjs": "^1.1.0",
    "nprogress": "^0.2.0",
    "path-to-regexp": "^6.2.1",
    "pinia": "2.0.12",
    "qs": "^6.11.1",
    "resize-observer-polyfill": "^1.5.1",
    "sortablejs": "^1.15.0",
    "vue": "^3.2.47",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6",
    "vue-types": "^4.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.5.1",
    "@commitlint/config-conventional": "^17.4.4",
    "@iconify/json": "^2.2.21",
    "@purge-icons/generated": "^0.9.0",
    "@types/crypto-js": "^4.1.1",
    "@types/lodash-es": "^4.17.7",
    "@types/mockjs": "^1.0.7",
    "@types/nprogress": "^0.2.0",
    "@types/qs": "^6.9.7",
    "@types/sortablejs": "^1.15.1",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/compiler-sfc": "^3.2.47",
    "@vue/test-utils": "^2.3.2",
    "cross-env": "^7.0.3",
    "cz-git": "^1.6.1",
    "czg": "^1.6.1",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.10.0",
    "fs-extra": "^10.1.0",
    "husky": "^7.0.4",
    "inquirer": "^8.2.5",
    "less": "^4.1.3",
    "lint-staged": "13.2.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "postcss-html": "^1.5.0",
    "postcss-less": "^6.0.0",
    "prettier": "^2.8.7",
    "rimraf": "^3.0.2",
    "stylelint": "^15.4.0",
    "stylelint-config-standard": "^32.0.0",
    "stylelint-order": "^6.0.3",
    "terser": "^5.16.8",
    "typescript": "^5.0.3",
    "unocss": "^0.51.8",
    "vite": "^4.2.1",
    "vite-plugin-mock": "^2.9.6",
    "vue-eslint-parser": "^9.1.1",
    "vue-tsc": "^1.2.0"
  },
  "packageManager": "pnpm@8.1.0",
  "engines": {
    "node": ">=16.15.1",
    "pnpm": ">=8.1.0"
  }
}
